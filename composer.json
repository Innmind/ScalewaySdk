{
    "name": "innmind/scaleway-sdk",
    "type": "library",
    "description": "Unofficial Scaleway SDK",
    "keywords": ["scaleway", "sdk"],
    "homepage": "http://github.com/Innmind/ScalewaySdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ScalewaySdk/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/time-continuum": "~3.4",
        "ramsey/uuid": "^3.8|^4.0",
        "innmind/http-transport": "~7.1",
        "innmind/json": "^1.1",
        "innmind/filesystem": "~7.3",
        "innmind/immutable": "~5.2",
        "innmind/ip": "~3.2",
        "innmind/url": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\ScalewaySdk\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\ScalewaySdk\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "innmind/object-graph": "~3.2",
        "vimeo/psalm": "~5.5",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0"
    }
}
