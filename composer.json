{
    "name": "innmind/scaleway-sdk",
    "type": "library",
    "description": "Unofficial Scaleway SDK",
    "keywords": ["scaleway", "sdk"],
    "homepage": "http://github.com/Innmind/ScalewaySdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ScalewaySdk/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/time-continuum": "~2.0",
        "ramsey/uuid": "^3.8|^4.0",
        "innmind/http-transport": "~5.0",
        "innmind/json": "^1.1",
        "innmind/filesystem": "~4.0",
        "innmind/immutable": "~3.3",
        "innmind/ip": "~2.0",
        "innmind/url": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\ScalewaySdk\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\ScalewaySdk\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "innmind/object-graph": "~2.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.16",
        "innmind/coding-standard": "^1.1"
    }
}
